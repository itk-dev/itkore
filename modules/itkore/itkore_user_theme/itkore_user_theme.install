<?php

/**
 * @file
 * Install, update, and uninstall functions for the itkore_user_theme module.
 */


/**
 * Implements hook_install().
 */
function itkore_user_theme_install() {
  user_role_grant_permissions('anonymous', array('access content','view the administration theme'));
  user_role_grant_permissions('authenticated', array('access content','view the administration theme'));
}